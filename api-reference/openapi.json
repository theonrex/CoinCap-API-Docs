            "schema": {
              "type": "string",
              "example": "poloniex"
            }
          },
          {
            "name": "baseSymbol",
            "in": "query",
            "description": "Returns markets containing the base symbol.",
            "required": false,
            "style": "form",
            "explode": true,
            "schema": {
              "type": "string",
              "example": "BTC"
            }
          },
          {
            "name": "quoteSymbol",
            "in": "query",
            "description": "Returns markets containing the quote symbol.",
            "required": false,
            "style": "form",
            "explode": true,
            "schema": {
              "type": "string",
              "example": "ETH"
